<launch>

    <!-- Load joint controller configurations from YAML file to parameter server -->
    <rosparam file="$(find crx_gazebo_control)/config/controller.yaml" command="load"/>

    <!-- load the controllers -->
    <node name="controller_spawner" pkg="controller_manager" 
            type="spawner" ns="crx_v1" output="screen"
            args="joint_a2b_effort_controller joint_b2c_effort_controller joint_d2e_effort_controller">
    </node>

    <node name="rqt_gui" pkg="rqt_gui" type="rqt_gui"/>

</launch>
